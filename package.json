  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm -ws run --if-present pre:dev && sst dev",
    "dev-aus": "npm -ws run --if-present pre:dev && sst dev --region ap-southeast-2",
    "build": "sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "storybook": "npm run storybook -w packages/web",
    "typecheck": "npm -ws run --if-present typecheck",
    "lint": "npm-run-all -p lint:workspaces",
    "lint:workspaces": "npm -ws run --if-present lint",
    "lint:fix": "npm-run-all lint:fix:workspaces",
    "lint:fix:workspaces": "npm -ws run --if-present lint:fix",
    "format": "npm -ws run --if-present format",
    "format:check": "npm -ws run --if-present format:check",
    "test": "npm -ws run --if-present test",
    "test:watch": "npm -ws run --if-present test:watch",
    "test:cloudflare": "npm -ws run --if-present test:cloudflare",
    "deploy:cloudflare": "npm -w packages/api/  run --if-present deploy:cloudflare:worker --environment=$npm_config_stage",
    "oas": "npm -ws run --if-present oas",
    "e2e:identity": "npm -ws run --if-present e2e:identity",
    "prepare": "husky install",
    "seed": "esbuild scripts/seed/index.ts --bundle --platform=node --outdir=dist && node dist/index.js"
  },
  "lint-staged": {
    "packages/api/redemptions/**/*.{js,jsx,mjs,ts,tsx,mts,json,md}": [
      "npm run -w packages/api/redemptions format:staged --"
    ],
    "packages/api/redemptions/**/*.{js,jsx,mjs,ts,tsx,mts}": [
      "npm run -w packages/api/redemptions lint:staged --"
    ],
    "packages/shared-ui/**/*.{js,jsx,mjs,ts,tsx,mts,json,md}": [
      "npm run -w packages/shared-ui format:staged --"
    ],
    "packages/shared-ui/**/*.{js,jsx,mjs,ts,tsx,mts}": [
      "npm run -w packages/shared-ui lint:staged --"
    ],
    "packages/api/offers-cms/**/*.{js,jsx,mjs,ts,tsx,mts,json,md}": [
      "npm run -w packages/api/offers-cms format:staged --"
    ],
    "packages/api/offers-cms/**/*.{js,jsx,mjs,ts,tsx,mts}": [
      "npm run -w packages/api/offers-cms lint:staged --"
    ]
  },
